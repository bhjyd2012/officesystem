<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hlsofficesystem.mapper.RegionalprofitMapper">

    <!-- 开启二级缓存 -->
    <!-- <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/> -->

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.hlsofficesystem.bean.Regionalprofit">
        <id column="id" property="id" />
        <result column="StoreNumber" property="StoreNumber" />
        <result column="StoreName" property="StoreName" />
        <result column="StoreSubjects" property="StoreSubjects" />
        <result column="StoreLastYearStatistics" property="StoreLastYearStatistics" />
        <result column="StoreTheYearStatistics" property="StoreTheYearStatistics" />
        <result column="StoreTheYearStatisticsDate" property="StoreTheYearStatisticsDate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, StoreNumber, StoreName, StoreSubjects, StoreLastYearStatistics, StoreTheYearStatistics, StoreTheYearStatisticsDate
    </sql>

</mapper>
